const jonas = {
    firstName: 'Jonas',
    LastName: "Pohuy",
    birthYear: 1991,
    job: 'teacher',
    friends: ["Michael", "Peter", "Steven"],
    hasDriversLicense: true,

    calcAge: function (birthYear) {
        console.log(this);
        return 2037 - this.birthYear;
    }

};

console.log(jonas.calcAge(jonas.birthYear));
